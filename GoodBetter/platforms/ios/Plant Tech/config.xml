<?xml version='1.0' encoding='utf-8'?>
<widget id="com.app.GoodBetter" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="ios-package" value="CDVBarcodeScanner" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <preference name="CameraUsesGeolocation" value="false" />
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="Ios_icon/icon-60@3x.png" width="180" />
    <icon height="60" src="Ios_icon/icon-60.png" width="60" />
    <icon height="120" src="Ios_icon/icon-60@2x.png" width="120" />
    <icon height="76" src="Ios_icon/icon-76.png" width="76" />
    <icon height="152" src="Ios_icon/icon-76@2x.png" width="152" />
    <icon height="40" src="Ios_icon/icon-40.png" width="40" />
    <icon height="80" src="Ios_icon/icon-40@2x.png" width="80" />
    <icon height="57" src="Ios_icon/icon.png" width="57" />
    <icon height="114" src="Ios_icon/icon@2x.png" width="114" />
    <icon height="72" src="Ios_icon/icon-72.png" width="72" />
    <icon height="144" src="Ios_icon//icon-72@2x.png" width="144" />
    <icon height="29" src="Ios_icon/icon-small.png" width="29" />
    <icon height="58" src="Ios_icon/icon-small@2x.png" width="58" />
    <icon height="50" src="Ios_icon/icon-50.png" width="50" />
    <icon height="100" src="Ios_icon/icon-50@2x.png" width="100" />
    <splash height="480" src="Ios_splash/Non-Retina.png" width="320" />
    <splash height="960" src="Ios_splash/Retina.png" width="640" />
    <splash height="1024" src="Ios_splash/Tablet (iPad).pngg" width="768" />
    <splash height="2048" src="Ios_splash/Retina (2x).png" width="1536" />
    <splash height="1136" src="Ios_splash/iPhone 5 Retina.png" width="640" />
    <splash height="1334" src="Ios_splash/iPhone 6.png" width="750" />
    <splash height="2208" src="Ios_splash/iPhone 6 Plus.png" width="1242" />
    <name>Plant Tech</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="portrait" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
</widget>
